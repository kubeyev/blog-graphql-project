type Post {
    id: ID,
    title: String,
    content: String,
    publishedDate: String
}
type Query {
    findAllPosts(count: Int): [Post]!
    getPost(id :ID): Post
    countPosts: Int!
}
type Mutation {
    createPost(input: PostInput!):Post
    updatePost(id: ID!, title: String, content: String): Post
    deletePost(id: ID!): Boolean!
}

input PostInput{
    title: String!,
    content: String!,
    publishedDate: String!
}